<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.login">
    <!--拍照-->
    <uses-permission android:name="android.permission.CAMERA" />
    <!--读写-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.PERMISSIONS_STORAGE"/>


    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/AppTheme.NoActionBar">

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.cameratest.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_path" />
        </provider>

        <activity android:name=".user.TaskRelease"></activity>
        <activity android:name=".user.UserWallet" />
        <activity android:name=".user.UserMainInterfaceActivity" />
        <activity android:name=".user.ModifyUserlInfo" />
        <activity android:name=".user.PersonalCenterActivity" />
        <activity android:name=".user.SkimInstitution">
            <intent-filter>
                <action android:name="android.intent.action.user.TitlePage"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".worker.workpersonalcenterActivity" />
        <activity android:name=".worker.WorkmaininterfaceAcitvity" />
        <activity android:name=".worker.workwalletActivity"/>

        <activity android:name=".community.CommunitymainActivity"/>

        <activity android:name=".IdentiChooseActivity" />
        <activity android:name=".EnrollActicity" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".UserLoginActivity" />
        <activity android:name=".CameraActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
